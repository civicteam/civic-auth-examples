{
  "name": "next15-turbopack-wallet-adapter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn dev-webpack",
    "dev-turbopack": "WEBPACK_ENABLE_SOLANA_WALLET_ADAPTER=true next dev --turbopack --port 3000",
    "dev-webpack": "WEBPACK_ENABLE_SOLANA_WALLET_ADAPTER=true next dev --port 3000",
    "typecheck": "tsc --noEmit",
    "build": "yarn typecheck && WEBPACK_ENABLE_SOLANA_WALLET_ADAPTER=true next build",
    "build-webpack": "next build",
    "build-turbopack": "WEBPACK_ENABLE_SOLANA_WALLET_ADAPTER=true next build --turbopack",
    "start-turbopack": "WEBPACK_ENABLE_SOLANA_WALLET_ADAPTER=true next start --turbopack --port 3000",
    "start": "WEBPACK_ENABLE_SOLANA_WALLET_ADAPTER=true next start --port 3000",
    "lint": "next lint"
  },
  "dependencies": {
    "@civic/auth-web3": "*",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "next": "^15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
