{
  "name": "next15-turbopack-wallet-adapter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run dev-webpack",
    "dev-turbopack": "next dev --turbopack --port 3000",
    "dev-webpack": "WEBPACK_ENABLE_SOLANA_WALLET_ADAPTER=true next dev --port 3000",
    "typecheck": "tsc --noEmit",
    "build": "yarn typecheck && WEBPACK_ENABLE_SOLANA_WALLET_ADAPTER=true next build",
    "build-webpack": "next build",
    "build-turbopack": "next build --turbopack",
    "start-turbopack": "next start --turbopack --port 3000",
    "start": "WEBPACK_ENABLE_SOLANA_WALLET_ADAPTER=true next start --port 3000",
    "lint": "next lint"
  },
  "dependencies": {
    "@civic/auth-web3": "*",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "bs58": "^6.0.0",
    "next": "^15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
