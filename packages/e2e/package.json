{
  "name": "e2e",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test:e2e": "cypress open",
    "test:e2e:run": "sh -c 'if [ -n \"$CYPRESS_RECORD_KEY\" ]; then cypress run --record --key $CYPRESS_RECORD_KEY; else cypress run; fi'",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:allure": "playwright test --reporter=allure-playwright",
    "allure:generate": "allure generate allure-results",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results",
    "test:allure": "yarn test:playwright:allure && yarn allure:serve"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^21.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.3",
    "@types/cypress": "^1.1.3",
    "cypress": "^13.15.0",
    "cypress-iframe": "^1.0.1",
    "cypress-wait-until": "^3.0.2",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.23",
    "allure-commandline": "^2.32.0",
    "allure-playwright": "^3.1.0",
    "dotenv": "^16.4.5"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "omitFiltered": true
  }
}
