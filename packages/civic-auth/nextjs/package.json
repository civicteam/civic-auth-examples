{
  "name": "civic-auth-nextjs-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:subpath": "BASE_PATH=/demo next dev",
    "dev:loginSuccessUrl": "LOGIN_SUCCESS_URL=/customSuccessRoute next dev --port 3000",
    "typecheck": "tsc --noEmit",
    "build": "yarn typecheck && next build",
    "start": "next start",
    "start:subpath": "BASE_PATH='/demo' next start",
    "start:loginSuccessUrl": "LOGIN_SUCCESS_URL='/customSuccessRoute' next start --port 3000",
    "lint": "next lint"
  },
  "dependencies": {
    "@civic/auth": "*",
    "next": "^15.2.6",
    "react": "^19.0.1",
    "react-dom": "^19.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^8",
    "eslint-config-next": "^15.2.2",
    "typescript": "^5"
  }
}
