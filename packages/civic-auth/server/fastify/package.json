{
  "name": "civic-auth-fastify-example",
  "version": "0.0.1",
  "description": "A sample fastify app with Civic Auth",
  "main": "dist/app.js",
  "type": "module",
  "private": "true",
  "scripts": {
    "start": "PORT=${PORT:-3003} node dist/app.js",
    "dev": "PORT=${PORT:-3003} NODE_OPTIONS=\"--loader ts-node/esm\" nodemon src/app.ts",
    "build": "tsc --outDir dist"
  },
  "dependencies": {
    "@civic/auth": "*",
    "@fastify/cookie": "^11.0.1",
    "bun": "^1.2.5",
    "dotenv": "^16.4.7",
    "fastify": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5.6.3"
  }
}
